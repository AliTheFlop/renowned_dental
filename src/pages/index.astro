import OurServices from '../components/OurServices.astro';
import Testimonials from '../components/Testimonials.astro';
import InfoBlocks from '../components/InfoBlocks.astro';
const pageTitle = 'Renowned Dental Clinic';
---

<BaseLayout pageTitle={pageTitle}>
    <MobileHeader />
    <Header />
    <Hero />
    <OurServices />
    <InfoBlocks />
    <Testimonials />
</BaseLayout>
